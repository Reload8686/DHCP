# Search for machine-generated header files in the build tree,
# and for normal headers in the source tree (${top_srcdir}).
# We only need to look in OS-specific subdirectories for the
# latter case, because there are no machine-generated OS-specific
# headers.

ISC_INCLUDES = -I/home/rener/dhcp-4.4.3-P1/bind/bind-9.11.36/lib/isc/include \
	-I${top_srcdir}/lib/isc \
	-I${top_srcdir}/lib/isc/include \
	-I${top_srcdir}/lib/isc/unix/include \
	-I${top_srcdir}/lib/isc/nothreads/include \
	-I${top_srcdir}/lib/isc/noatomic/include

ISCCC_INCLUDES = -I/home/rener/dhcp-4.4.3-P1/bind/bind-9.11.36/lib/isccc/include \
       -I${top_srcdir}/lib/isccc/include

ISCCFG_INCLUDES = -I/home/rener/dhcp-4.4.3-P1/bind/bind-9.11.36/lib/isccfg/include \
       -I${top_srcdir}/lib/isccfg/include

DNS_INCLUDES = -I/home/rener/dhcp-4.4.3-P1/bind/bind-9.11.36/lib/dns/include \
	-I${top_srcdir}/lib/dns/include

IRS_INCLUDES = -I/home/rener/dhcp-4.4.3-P1/bind/bind-9.11.36/lib/irs/include \
	-I${top_srcdir}/lib/irs/include

LWRES_INCLUDES = -I/home/rener/dhcp-4.4.3-P1/bind/bind-9.11.36/lib/lwres/include \
	-I${top_srcdir}/lib/lwres/unix/include \
	-I${top_srcdir}/lib/lwres/include

BIND9_INCLUDES = -I/home/rener/dhcp-4.4.3-P1/bind/bind-9.11.36/lib/bind9/include \
	-I${top_srcdir}/lib/bind9/include

TEST_INCLUDES = \
	-I${top_srcdir}/lib/tests/include
